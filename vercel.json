{
  "version": 2,
  "buildCommand": "pnpm install && pnpm run build",
  "outputDirectory": "dist/public",
  "routes": [
    {
      "src": "/api/trpc/(.*)",
      "dest": "/api/trpc/[...trpc]"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
